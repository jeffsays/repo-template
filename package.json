{
  "name": "repo-template",
  "version": "0.0.0",
  "description": "Repository Template",
  "homepage": "https://github.com/jeffsays/repo-template",
  "bugs": {
    "url": "https://github.com/jeffsays/repo-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffsays/repo-template.git"
  },
  "license": "MIT",
  "author": "Jeff Beck",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/@types/index.d.ts",
      "default": "./lib/index.js",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/@types",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "./lib/**",
    "./package.json"
  ],
  "scripts": {
    "build": "yarn run clean && build:tsconfig",
    "build:all": "yarn run build:configs && yarn run build:eslint && yarn run build:scripts && yarn run build:test && yarn run build:tsconfig",
    "build:configs": "tsc -p tsconfig.configs.json",
    "build:eslint": "tsc -p tsconfig.eslint.json",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "build:test": "tsc -p tsconfig.test.json",
    "build:tsconfig": "tsc -p tsconfig.json",
    "clean": "shx rm -rf .turbo bin dist lib coverage",
    "eslint:check": "eslint . --color --fix-dry-run",
    "eslint:inspect": "eslint --inspect-config",
    "eslint:write": "eslint . --color --fix",
    "git:commit": "git add package.json .yarn/versions && git commit -m v${npm_package_version}",
    "git:push": "git push --follow-tags",
    "git:tag": "git tag -a v${npm_package_version} -m v${npm_package_version}",
    "prepack": "yarn build",
    "prepare": "is-ci || husky",
    "prettier:check": "node --import tsx node_modules/prettier/bin/prettier.cjs --ignore-path ./.prettierignore . --check",
    "prettier:list-different": "node --import tsx node_modules/prettier/bin/prettier.cjs --ignore-path ./.prettierignore . --list-different",
    "prettier:write": "node --import tsx node_modules/prettier/bin/prettier.cjs --ignore-path ./.prettierignore . --write",
    "publish": "yarn npm publish --access public",
    "publish:major": "yarn version major && yarn publish:sync",
    "publish:minor": "yarn version minor && yarn publish:sync",
    "publish:patch": "yarn version patch && yarn publish:sync",
    "publish:sync": "yarn run git:commit && yarn run git:tag && yarn git:push",
    "test:jest": "node --experimental-vm-modules node_modules/.bin/jest --color --coverage"
  },
  "prettier": "@jeffsays/prettier",
  "devDependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@eslint/config-inspector": "^1.3.0",
    "@eslint/markdown": "^7.5.1",
    "@jeffsays/cfn-lint": "^0.0.0",
    "@jeffsays/commitlint": "^0.0.3",
    "@jeffsays/editorconfig": "^0.0.0",
    "@jeffsays/eslint": "^0.0.30",
    "@jeffsays/lint-staged": "^0.0.14",
    "@jeffsays/markdownlint-cli2": "^0.0.3",
    "@jeffsays/prettier": "^0.0.25",
    "@jeffsays/shellcheck": "^0.0.0",
    "@jeffsays/tsconfig": "^0.0.5",
    "@jeffsays/tsdoc": "^0.0.0",
    "@jeffsays/yamllint": "^0.0.0",
    "@jest/globals": "^30.2.0",
    "@types/babel__preset-env": "^7.10.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "commander": "^14.0.2",
    "commitlint": "^20.1.0",
    "eslint": "^9.39.1",
    "get-tsconfig": "^4.13.0",
    "git-describe": "^4.1.1",
    "git-list-updated": "^1.2.1",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.10.3+sha512.c38cafb5c7bb273f3926d04e55e1d8c9dfa7d9c3ea1f36a4868fa028b9e5f72298f0b7f401ad5eb921749eb012eb1c3bb74bf7503df3ee43fd600d14a018266f",
  "engines": {
    "node": ">=24.11.0 || >=25.1.0",
    "yarn": ">=4"
  },
  "publishConfig": {
    "access": "public"
  }
}
